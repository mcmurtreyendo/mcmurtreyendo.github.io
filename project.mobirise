        "footer_custom": "<script>\n/* Insurance pricing updater for Mobirise blocks\n   - Leave ONLY the <select id=\"insuranceSelect\"> in your section.\n   - Put THIS script into Page/Site Footer so it actually executes. */\n(function () {\n  // 1) EDIT YOUR RATES HERE\n  var PRICING = {\n    \"Without insurance\": { \"Evaluation\": 322, \"Root Canals\": 1080, \"Root Surgery\": 1120 },\n    \"UHC\":               { \"Evaluation\": 120, \"Root Canals\": 720,  \"Root Surgery\": 800  },\n    \"Delta\":             { \"Evaluation\": 140, \"Root Canals\": 760,  \"Root Surgery\": 840  },\n    \"Guardian\":          { \"Evaluation\": 160, \"Root Canals\": 790,  \"Root Surgery\": 880  }\n  };\n\n  // ---- utilities\n  function $(sel, root){ return (root || document).querySelector(sel); }\n  function $all(sel, root){ return Array.from((root || document).querySelectorAll(sel)); }\n  function format(n){ return Number(n).toLocaleString('en-US'); }\n\n  // Build once when the selector exists\n  function init() {\n    var select = document.getElementById('insuranceSelect');\n    if (!select) return false;\n\n    var section = select.closest('section') || select.closest('.pricing2') || document;\n    var plans = {}; // title -> { priceEl, termEl, originalPriceHTML, originalTermHTML }\n\n    $all('.plan', section).forEach(function (plan) {\n      var titleEl = plan.querySelector('.plan-title');\n      var priceEl = plan.querySelector('.plan-price .price');\n      var termEl  = plan.querySelector('.plan-price .price-term');\n      if (!titleEl || !priceEl || !termEl) return;\n\n      var title = titleEl.textContent.replace(/\\s+/g,' ').trim();\n      plans[title] = {\n        priceEl: priceEl,\n        termEl: termEl,\n        originalPriceHTML: priceEl.innerHTML, // snapshot originals to restore later\n        originalTermHTML: termEl.innerHTML\n      };\n    });\n\n    function setPlanPrice(title, amount) {\n      var p = plans[title];\n      if (!p) return;\n\n      // Always start from the original HTML so replacements never \"drift\"\n      var html = p.originalPriceHTML;\n      var out  = html;\n\n      // Replace \"$####*\" anywhere (covers \"Starting at $1080*\")\n      out = out.replace(/\\$\\s*[\\d,]+(?:\\.\\d{1,2})?\\s*\\*/g, '$' + format(amount) + '*');\n\n      // If nothing changed but text mentions \"Starting at\", rebuild minimal structure\n      if (out === html && /Starting\\s*at/i.test(html.replace(/<[^>]+>/g,''))) {\n        out = '<b>Starting at</b><br><b>$' + format(amount) + '*</b>';\n      }\n\n      // If still nothing changed (rare: no $ found), just show the amount\n      if (out === html) {\n        out = '<b>$' + format(amount) + '*</b>';\n      }\n\n      p.priceEl.innerHTML = out;\n    }\n\n    function updateAll(insurer) {\n      var table = PRICING[insurer] || PRICING['Without insurance'];\n      Object.keys(plans).forEach(function (title) {\n        var p = plans[title];\n        if (table[title] != null) {\n          setPlanPrice(title, table[title]);\n          p.termEl.innerHTML = (insurer === 'Without insurance')\n            ? p.originalTermHTML\n            : '<b>With ' + insurer + '</b>';\n        } else {\n          // Plans not in the table (e.g., \"Free\") are restored to original\n          p.priceEl.innerHTML = p.originalPriceHTML;\n          p.termEl.innerHTML  = p.originalTermHTML;\n        }\n      });\n    }\n\n    // Initial + change\n    updateAll(select.value);\n    select.addEventListener('change', function(){ updateAll(this.value); });\n\n    return true;\n  }\n\n  // Try now; if not ready (Mobirise may inject later), watch the DOM until it appears\n  if (!init()) {\n    var tried = 0, maxTries = 50; // ~5s\n    var iv = setInterval(function(){\n      tried++;\n      if (init() || tried >= maxTries) clearInterval(iv);\n    }, 100);\n    // Plus MutationObserver for SPA-like loads\n    var mo = new MutationObserver(function(){\n      if (init()) mo.disconnect();\n    });\n    mo.observe(document.documentElement, { childList: true, subtree: true });\n    setTimeout(function(){ try { mo.disconnect(); } catch(e){} }, 10000);\n  }\n})();\n</script>",
